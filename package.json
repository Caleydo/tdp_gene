{
  "name": "targid_common",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/targid_common",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_data_sql": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "targidView",
          "type": "targidView",
          "id": "ensembl_org",
          "name": "ensembl.org",
          "category": "static",
          "site": "//apr2013.archive.ensembl.org/id/{gene}",
          "argument": "gene",
          "idtype": "Ensembl",
          "selection": "multiple"
        },
        {
          "type": "targidView",
          "id": "gene_card",
          "name": "genecards.org",
          "category": "static",
          "site": "//www.genecards.org/cgi-bin/carddisp.pl?id_type=esembl&id={gene}",
          "argument": "gene",
          "idtype": "Ensembl",
          "selection": "multiple"
        },
        {
          "type": "targidView",
          "id": "cansar",
          "name": "cansar.icr.ac.uk",
          "category": "static",
          "site": "//cansar.icr.ac.uk/cansar/molecular-targets/{gene}/",
          "argument": "gene",
          "idtype": "UniProt",
          "selection": "multiple"
        },
        {
          "type": "targidView",
          "id": "uniprot",
          "name": "uniprot.org",
          "category": "static",
          "site": "//www.uniprot.org/uniprot/{gene}/",
          "argument": "gene",
          "idtype": "UniProt",
          "selection": "multiple"
        },
        {
          "type": "targidView",
          "id": "cosmic",
          "name": "COSMIC",
          "category": "static",
          "site": "//cancer.sanger.ac.uk/cell_lines/sample/overview?name={cellline}",
          "argument": "cellline",
          "idtype": "Cellline",
          "selection": "multiple"
        },
        {
          "type": "targidView",
          "id": "targetvalidation",
          "name": "targetvalidation.org",
          "category": "static",
          "site": "//www.targetvalidation.org/target/{gene}",
          "argument": "gene",
          "idtype": "Ensembl",
          "selection": "multiple"
        }
      ]
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
